{
    "singleQuote": false,
    "trailingComma": "all",
    "printWidth": 80,
    "tabWidth": 4,
    "endOfLine": "auto",
    "plugins": ["@ianvs/prettier-plugin-sort-imports"],
    "importOrder": [
        "^(react/(.*)$)|^(react$)",
        "^(next/(.*)$)|^(next$)",
        "^@radix-ui/(.*)$",
        "<THIRD_PARTY_MODULES>",
        "^[.]",
        "^@/lib/(.*)$",
        "^@/components/(.*)$",
        "^@/(.*)$",
        "<TYPES>",
        ""
    ]
}
